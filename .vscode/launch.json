{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug xtemplate",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": true,
            "program": "${workspaceFolder}/cmd",
            "cwd": "${workspaceFolder}/test",
            "args": [
                "--loglevel",
                "-4",
                "--config-file",
                "config.json"
            ]
        },
        {
            "name": "Debug xtemplate-caddy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "showLog": true,
            "program": "${workspaceFolder}/test/caddy",
            "cwd": "${workspaceFolder}/test",
            "args": [
                "run",
                "--config",
                "caddy.json"
            ]
        },
    ]
}